{
    "name": "indigophp/proton-tools",
    "description": "Helpers to create awesome Proton applications",
    "license": "MIT",
    "keywords": ["proton"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "alexbilbie/proton": "~1.0"
    },
    "require-dev": {
        "indigophp/guardian": "dev-master"
    },
    "suggest": {
        "indigophp/guardian": "Used for auth controller"
    },
    "autoload": {
        "psr-4": {
            "Proton\\Tools\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
